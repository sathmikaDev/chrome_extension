{
  "manifest_version": 3,
  "name": "Sheet Notifier",
  "version": "1.0",
  "description": "Get notified when your sheet updates!",
  "permissions": [
    "identity",
    "storage",
    "activeTab",
    "scripting",
    "notifications"
  ],
  "oauth2": {
    "client_id": "220501934503-3kg9rm31hulq11s4brheo1rn86t6imtd.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/spreadsheets.readonly",
      "https://www.googleapis.com/auth/userinfo.email"
    ]
  },
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": [
    "https://docs.google.com/spreadsheets/*",
    "https://script.google.com/*"
  ]
}